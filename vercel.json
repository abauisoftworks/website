{
  "cleanUrls": false,
  "headers": [
    {
      "source": "/raw/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/raw/(.*)", "destination": "/raw/$1.lua" },
    { "source": "/raw/(.*)", "destination": "/raw/$1.txt" },
    { "source": "/raw", "destination": "/raw/index.html" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/api", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/soon.html" }
  ]
}
