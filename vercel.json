{
  "headers": [
    {
      "source": "/raw/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/raw/(.*)", "destination": "/raw/$1" }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}
