{
  "cleanUrls": false,
  "headers": [
    {
      "source": "/raw/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/raw/(.*).lua", "destination": "/raw/$1.lua" },
    { "source": "/raw/(.*).txt", "destination": "/raw/$1.txt" }
  ]
}
